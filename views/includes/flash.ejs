<% if(success.length>0) {%>
    <div class="alert alert-success alert-dismissible fade show col-6 offset-3 fade-in" role="alert">
        <%=success%>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    <%}%>

    <% if(error.length>0) {%>
        <div class="alert alert-danger alert-dismissible fade show col-6 offset-3 fade-in" role="alert">
            <%=error%>
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
        <%}%>
